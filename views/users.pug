extends layouts/main-layout.pug
block content
    br
    .container
        table.table
            thead
                tr
                    th(scope="col") Username
                    th(scope="col") First Name
                    th(scope="col") Last Name
                    th(scope="col") Role
            tbody
                each user in items
                    tr
                        th(scope="row") #{user.username}
                        th #{user.firstName}
                        th #{user.lastName}
                        th #{user.role}
